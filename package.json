    "scripts": {
        "test": "react scripts test",
        "copyfiles": "cp -r build/public/static/images public/static/ && rm -rf build && mkdir build && cp -r public build && cp ./server/server.config.js ./build && prettier --write src/",
        "webpack:client": "webpack --env development --mode development --config webpack.client.js --watch",
        "webpack:server": "webpack --env development --mode development --config webpack.server.js --watch",
        "webpack:run": "nodemon --watch build --exec node build/bundle.js",
        "build:client": "npm run copyfiles && webpack --mode production --env production --config webpack.client.js",
        "build:server": "webpack --env production --mode production --config webpack.server.js",
        "build": "npm run copyfiles; npm-run-all --parallel build:*",
        "copystatic": "cp -r build/public/static/fonts/ /home/eobgycvo/public_html/static/ && cp -r build/public/static/scripts/ /home/eobgycvo/public_html/static/ && cp -r build/public/static/styles/ /home/eobgycvo/public_html/static/ && cp -r build/public/static/*.js /home/eobgycvo/public_html/static && cp -r build/public/static/*.map /home/eobgycvo/public_html/static",
        "prod": "pm2 start build/server.config.js",
        "stop:prod": "pm2 stop myhome & pm2 delete myhome",
        "make-dev": "concurrently -n frontend,server,node \"npm run webpack:client\" \"npm run webpack:server\" \"npm run webpack:run\"",
        "dev": "npm run webpack:run"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
